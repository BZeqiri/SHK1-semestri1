import java.util.Scanner;
public class Pjestimi {
    public static void main (String[]bz) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Shtyp nje numer :");
        long p = sc.nextLong();
        System.out.println("Shtyp nje numer tjeter :");
        long q = sc.nextLong();
        System.out.println("Rezultati eshte :" + ((double)p/q));
        
    
    }
    
}
